{
    "name": "myapp-client-bundle",
    "version": "1.0.0",
    "description": "This is client-side scripts bundle for MyApp",
    "private": true,
    "scripts": {
      "build": "webpack --mode=development",
      "build:prod": "webpack --mode=production",
      "watch": "webpack --watch",
      "css:build": "npx tailwindcss -i ../wwwroot/css/site.css -o ../wwwroot/css/styles.css --minify",
      "css:watch": "npx tailwindcss --watch -i ../wwwroot/css/site.css -o ../wwwroot/css/styles.css --minify"
    },
    "devDependencies": {
      "ts-loader": "^9.2.5",
      "typescript": "^4.4.3",
      "webpack": "^5.52.1",
      "webpack-cli": "^4.8.0"
    },
    "dependencies": {
    }
  }