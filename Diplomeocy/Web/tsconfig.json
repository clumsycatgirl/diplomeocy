{
	"compileOnSave": true,
	"compilerOptions": {
		"noImplicitAny": false,
		"noEmitOnError": true,
		"noImplicitReturns": true,
		"removeComments": false,
		"sourceMap": true,
		"target": "es6",
		"outDir": "wwwroot/js",
		"moduleResolution": "Node",
		"esModuleInterop": true
	},
	"include": ["Scripts/**/*"]
}
